      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "cm:latency:p50_5m",
          "legendFormat": "p50 — {{exchange}}",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "cm:latency:p99_5m",
          "legendFormat": "p99 — {{exchange}}",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "cm:latency:p999_5m",
          "legendFormat": "p99.9 — {{exchange}}",
          "refId": "C"
        }
      ],
      "title": "Current Latency Summary",
      "type": "stat"
